{
  "name": "bplustree",
  "version": "1.1.0",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --check-leaks test/bplustree.js && npm run build",
    "test-full": "mocha --compilers js:babel-core/register --check-leaks test/bplustree.js test/full.js && npm run build",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- test/bplustree.js",
    "coverage-full": "babel-node node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- test/bplustree.js test/full.js",
    "doc": "jsdoc lib -d docs",
    "build": "babel lib -d dist"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "benchmark": "^1.0.0",
    "bplus-index": "0.0.6",
    "colors": "^1.1.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "faker": "^3.0.1",
    "isparta": "^4.0.0",
    "lodash": "^3.10.1",
    "log-anything": "^1.0.1",
    "microtime": "^2.0.0",
    "mocha": "^2.3.4",
    "sorted-map": "^0.1.8"
  },
  "description": "B+ tree",
  "main": "dist/bplustree.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vhf/bplustree.git"
  },
  "keywords": [
    "B+",
    "plus",
    "tree"
  ],
